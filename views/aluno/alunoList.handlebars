<table border="1px">

    <tr>
        <td> Nome </td>
        <td> Pais </td>
        <td> Endereco</td>
        <td> Residencia </td>
        <td> Editar </td>
        <td> Deletar </td>
    </tr>

    {{#each alunos}}

    <tr>
        <td>{{nome}}</td>
        <td>{{pais}}</td>
        <td>{{endereco}}</td>
        
        <td><image width="50px" height="50px" src="uploads/{{residencia}}"></td>
        <td><a href="alunoEdit/{{_id}}"> Editar </a></td>
        <td><a href="alunoDelete/{{_id}}"> Deletar </a></td>
    </tr>

    {{/each}}

</table>
